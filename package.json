    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "5.2.0",
    "react-test-renderer": "^17.0.2",
    "regenerator-runtime": "^0.13.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.8",
    "rollup-plugin-terser": "^7.0.1",
    "source-map-loader": "^0.2.4",
    "storybook-addon-performance": "^0.16.1",
    "ts-node": "^8.8.2",
    "typescript": "^4.1.0",
    "url-loader": "4.1.0",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11"
  },
  "resolutions": {
    "caniuse-lite": "^1.0.30001135",
    "@types/react": "^16.9.56"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "workspaces": [
    "packages/*",
    "website"
  ],
  "version": "0.0.0"
}
